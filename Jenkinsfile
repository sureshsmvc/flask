pipeline {
    agent any

    stages {
      stage('Generate') {
          steps {
              checkout scm
              stash 'source'
          }
        }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
